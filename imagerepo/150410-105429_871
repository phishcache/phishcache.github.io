	function tabview_p(n) { 
		for(var i = 1; i < 3; i++) { 
			var obj = document.getElementById("mtab-bbs00"+i); 
			var img = document.getElementById("tabmimg00"+i); 
			if ( n == i ) { 
				obj.style.display = "block"; 
				img.src = "/seoul/_res/img/btn_mtitle010" + i + "_on.gif"; 
			} else { 
				obj.style.display = "none"; 
				img.src = "/seoul/_res/img/btn_mtitle010" + i + "_off.gif"; 
			} 
		} 
	} 
	
	</script> 
